pipeline {
          agent any
          stages {
                  stage('One'){
                  steps {
                        echo('Hi, welcome to pipeline demo...')
                  }
                  }
                  stage('Two'){
                  steps{
                        echo('sample testingn of stage 2')
                  }
                  }
                  stage('Three'){
                  steps {
                         echo('Thanks for using Jenkins Pipeline')
                  }
                  }
                 }
  }
                        
                        
                      
